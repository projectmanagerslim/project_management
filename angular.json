{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "slim_pfe": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/slim_pfe",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.css",

"src/assets/plugins/bootstrap/css/bootstrap.min.css",
"src/assets/plugins/jvectormap/jquery-jvectormap-2.0.3.min.css",
"src/assets/plugins/charts-c3/plugin.css",
"src/assets/plugins/morrisjs/morris.min.css",
"src/assets/css/style.min.css",
"src/assets/plugins/jquery-datatable/dataTables.bootstrap4.min.css"





            ],
            "scripts": [
"src/assets/bundles/libscripts.bundle.js", 
"src/assets/bundles/vendorscripts.bundle.js", 
"src/assets/bundles/jvectormap.bundle.js", 
"src/assets/bundles/sparkline.bundle.js", 
"src/assets/bundles/c3.bundle.js",
"src/assets/bundles/mainscripts.bundle.js",
"src/assets/js/pages/index.js",

"src/assets/bundles/libscripts.bundle.js", 
"src/assets/bundles/vendorscripts.bundle.js",  
"src/assets/bundles/datatablescripts.bundle.js",
"src/assets/plugins/jquery-datatable/buttons/dataTables.buttons.min.js",
"src/assets/plugins/jquery-datatable/buttons/buttons.bootstrap4.min.js",
"src/assets/plugins/jquery-datatable/buttons/buttons.colVis.min.js",
"src/assets/plugins/jquery-datatable/buttons/buttons.flash.min.js",
"src/assets/plugins/jquery-datatable/buttons/buttons.html5.min.js",
"src/assets/plugins/jquery-datatable/buttons/buttons.print.min.js",
"src/assets/js/pages/tables/jquery-datatable.js"




            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "slim_pfe:build:production"
            },
            "development": {
              "buildTarget": "slim_pfe:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "slim_pfe:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "8be52029-8ed5-48ce-82ce-a45dbf6e8228"
  }
}
